{
  "name": "redocs",
  "version": "0.0.0",
  "description": "Powerful documentation utilities for React",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint examples/** src/**",
    "test": "npm run typecheck && npm run lint",
    "typecheck": "flow check --all",
    "docs": "react-docgen examples/ --pretty -o ./test.json",
    "build": "babel src/ -d lib/ --source-maps",
    "build:watch": "babel src/ --out-dir lib/ --source-maps --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lystable/redocs.git"
  },
  "keywords": [
    "documentation",
    "react"
  ],
  "author": "Lystable Dev <dev@lystable.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lystable/redocs/issues"
  },
  "homepage": "https://github.com/lystable/redocs",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "4.1.8",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-runtime": "6.6.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "babel-register": "6.7.2",
    "babel-runtime": "6.6.1",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "5.0.0",
    "eslint-config-lystable": "3.0.1",
    "eslint-plugin-flow-vars": "^0.2.1",
    "eslint-plugin-no-implicit-side-effects": "^1.0.0",
    "eslint-plugin-react": "3.14.0"
  },
  "dependencies": {
    "babel-generator": "^6.7.2",
    "babel-traverse": "^6.7.4",
    "babel-types": "^6.7.2",
    "babylon": "^6.7.0",
    "jsxstyle": "0.0.18",
    "lodash": "^4.7.0",
    "react": "^0.14.8",
    "react-docgen": "^2.7.0",
    "react-dom": "^0.14.8",
    "recompose": "^0.15.1"
  }
}
